{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/status",
            "destination": "/api/index.py"
        },
        {
            "source": "/brand(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/widget/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/static/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/health",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/vallit-site/$1"
        }
    ]
}