{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "vallit-site/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/app.py"
        },
        {
            "src": "/dashboard(.*)",
            "dest": "/app.py"
        },
        {
            "src": "/login",
            "dest": "/app.py"
        },
        {
            "src": "/register",
            "dest": "/app.py"
        },
        {
            "src": "/setup-password",
            "dest": "/app.py"
        },
        {
            "src": "/admin(.*)",
            "dest": "/app.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/app.py"
        },
        {
            "src": "/company(.*)",
            "dest": "/app.py"
        },
        {
            "src": "/(.*)",
            "dest": "/vallit-site/$1"
        }
    ]
}